<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" >
            <size
                android:height="50dp"
                android:width="50dp"/>
        </shape>
    </item>
    <item>
        <inset>
            <shape
                android:shape="oval"
                android:useLevel="false">
                <stroke
                    android:width="3dp"
                    android:dashGap="132dp"
                    android:dashWidth="90dp"
                    android:color="@color/loading_progress_bar_outer_circle" />
            </shape>
        </inset>
    </item>
</layer-list>